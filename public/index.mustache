	<div id="team" class="section md-padding bg-grey">
		<div class="container">
			<div class="row">
				<div class="section-header text-center">
					<h2 class="title">{{#t}}Our Team{{/t}}</h2>
				</div>

                <div class="col-sm-12 owl-carousel owl-theme" id="team-holder">
                    {{#members}}
                        <div class="team">
                            <div class="team-img">
                                <img class="img-responsive" src="{{image}}" alt="{{name}}">
                                <div class="overlay">
                                    <div class="team-social">
                                        {{#linkedin}}<a href="{{linkedin}}"><i class="fa fa-linkedin"></i></a>{{/linkedin}}
                                        {{#website}}<a href="{{website}}"><i class="fa fa-link"></i></a>{{/website}}
                                        {{#email}}<a href="mailto:{{email}}"><i class="fa fa-at"></i></a>{{/email}}
                                    </div>
                                </div>
                            </div>
                            <div class="team-content" style="white-space: normal">
                                <h3>{{name}}</h3>
                                <span>{{occupation}}</span>
                            </div>
                        </div>
                    {{/members}}
                </div>
			</div>
		</div>
	</div>


    <div id="join" class="section md-padding">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <iframe id="video-join" width="100%" height="400px" src="https://www.youtube.com/embed/9H7CN8g5DB0?rel=0&amp;controls=0&amp;showinfo=0" frameborder="0"></iframe>
                </div>
                <div class="col-md-6">
                    <div class="section-header">
                        <h2 class="title">{{#t}}Join to our Team{{/t}}</h2>
                    </div>
                    {{#t}}join_text{{/t}}
                </div>
            </div>
        </div>
    </div>


    <div id="sponsors" class="section md-padding">
        <div class="bg-img" style="background-image: url('./img/backgrounds/03.jpg');">
            <div class="overlay"></div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div id="sponsors-slider" class="owl-carousel owl-theme">

                        {{#sponsors}}
                        <div class="sponsor">
                            <div class="sponsor-meta">
                                <img src="{{image}}" alt="">
                                <p>{{title}}</p>
                            </div>
                        </div>
                        {{/sponsors}}
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div id="contact" class="section md-padding">
        <div class="container">
            <div class="row">
                <div class="section-header text-center">
                    <h2 class="title">{{#t}}Contact Us{{/t}}</h2>
                </div>
                <div class="col-sm-4">
                    <div class="contact">
                        <i class="fa fa-phone"></i>
                        <h3>Phone</h3>
                        <p>{{phone}}</p>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="contact">
                        <i class="fa fa-envelope"></i>
                        <h3>Email</h3>
                        <p>{{email}}</p>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="contact">
                        <i class="fa fa-map-marker"></i>
                        <h3>Address</h3>
                        <p>{{address}}</p>
                    </div>
                </div>
                <div class="col-md-8 col-md-offset-2">
                    <form class="contact-form" name="contact" data-netlify="true">
                        <div>
                            <input type="text" name="name" class="input" placeholder="{{#t}}Name{{/t}}">
                            <input type="email" name="email" class="input" placeholder="{{#t}}Email{{/t}}">
                            <input type="text" name="subject" class="input" placeholder="{{#t}}Subject{{/t}}">
                            <textarea name="message" class="input" placeholder="{{#t}}Message{{/t}}"></textarea>
                            <button class="main-btn">{{#t}}Send message{{/t}}</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>


	<footer id="footer" class="sm-padding bg-dark">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="footer-logo">
						<a href="/"><img src="img/logo-alt.png" alt="logo"></a>
					</div>
					<ul class="footer-follow">
						<li><a target="_blank" href="https://www.linkedin.com/company/memristor/"><i class="fa fa-linkedin"></i></a></li>
                        <li><a target="_blank" href="https://sketchfab.com/memristor/models"><i class="fa fa-cube"></i></a></li>
                        <li><a target="_blank" href="https://github.com/Memristor-Robotics/"><i class="fa fa-github"></i></a></li>
						<li><a target="_blank" href="https://twitter.com/MemristorRobots"><i class="fa fa-twitter"></i></a></li>
						<li><a target="_blank" href="https://www.youtube.com/channel/UC1LYpli22VV5Ndgr9rjU66A"><i class="fa fa-youtube"></i></a></li>
						<li><a target="_blank" href="https://www.facebook.com/MemristorRobotics/"><i class="fa fa-facebook"></i></a></li>
						<li><a target="_blank" href="https://www.instagram.com/MemristorRobotics/"><i class="fa fa-instagram"></i></a></li>
					</ul>
					<div class="footer-copyright">
						<p>Copyright Â© {{ year }}. All Rights Reserved.<br />
                            Designed by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                        </p>
					</div>
				</div>
			</div>
		</div>
	</footer>

	<script type="text/javascript" src="js/jquery.min.js"></script>
	<script type="text/javascript" src="js/bootstrap.min.js"></script>
    	<script type="text/javascript" src="js/owl.carousel.min.js"></script>
	<script type="text/javascript" src="js/jquery.magnific-popup.js"></script>
    	<script type="text/javascript" src="js/main.js"></script>
    	<script src="https://apps.elfsight.com/p/platform.js" defer></script>
</body>
</html>
