<!DOCTYPE html>
<html lang="{{ language.code }}">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
    	<meta name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' />

	<title>{{title}} | {{#t}}description{{/t}}</title>
    	<meta name="description" content="{{#t}}description_long{{/t}}">
    	<meta name="keywords" content="{{#t}}keywords{{/t}}">
	<meta name="yandex-verification" content="db9ea2ee6c71c319" />

	<link rel="shortcut icon" href="img/favicon.ico" type="image/x-icon">
	<link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7CVarela+Round" rel="stylesheet">
	<link type="text/css" rel="stylesheet" href="css/bootstrap.min.css" />
	<link type="text/css" rel="stylesheet" href="css/owl.carousel.css" />
	<link type="text/css" rel="stylesheet" href="css/owl.theme.default.css" />
	<link type="text/css" rel="stylesheet" href="css/magnific-popup.css" />
	<link rel="stylesheet" href="css/font-awesome.min.css">
	<link type="text/css" rel="stylesheet" href="css/style.css" />

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-114746710-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-114746710-1');
    </script>
</head>

<body>
	<header id="home">
		<div class="bg-img" style="background-image: url('./img/backgrounds/MemristorTeam.jpg');">
			<div class="overlay"></div>
		</div>
		<nav id="nav" class="navbar nav-transparent">
			<div class="container">
				<div class="navbar-header">
					<div class="navbar-brand">
						<a href="{{ url }}">
							<img class="logo" src="img/logo.png" alt="logo">
							<img class="logo-alt" src="img/logo-alt.png" alt="logo">
						</a>
					</div>
					<div class="nav-collapse">
						<span></span>
					</div>
				</div>
				<ul class="main-nav nav navbar-nav navbar-right">
					<li><a href="#home">{{#t}}Home{{/t}}</a></li>
					<li><a href="#about">{{#t}}About{{/t}}</a></li>
					<li><a href="#rd">{{#t}}R&D{{/t}}</a></li>
                    <li><a href="#gallery">{{#t}}Gallery{{/t}}</a></li>
					<li><a href="#team">{{#t}}Team{{/t}}</a></li>
                    <li><a href="#join">{{#t}}Join{{/t}}</a></li>
                    <li><a href="#sponsors">{{#t}}Sponsors{{/t}}</a></li>
                    <li><a href="#contact">{{#t}}Contact{{/t}}</a></li>
                    <li class="has-dropdown"><a href="#home">{{ language.code_up }}</a>
                        <ul class="dropdown">
                            {{#languages}}
                            <li><a href="{{file}}">{{code_up}}</a></li>
                            {{/languages}}
                        </ul>
                    </li>
				</ul>
			</div>
		</nav>
		<div class="home-wrapper">
			<div class="container">
				<div class="row">
					<div class="col-md-10 col-md-offset-1">
						<div class="home-content">
							<h1 class="white-text">{{#t}}description{{/t}}</h1>
                            <p class="white-text">{{#t}}description_long{{/t}}</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>


    <div id="about" class="section md-padding bg-grey">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="section-header">
                        <h2 class="title">{{#t}}About Us{{/t}}</h2>
                    </div>

                    {{#t}}about_text{{/t}}
                </div>
                <div class="col-md-6">
                    <iframe width="100%" height="400px" src="https://sketchfab.com/models/5afd6b9efac3402ca44c9631caa03139/embed?autospin=0.2&amp;autostart=1" frameborder="0" allowvr allowfullscreen mozallowfullscreen="true" webkitallowfullscreen="true" onmousewheel=""></iframe>
                </div>
            </div>
        </div>
    </div>


    <div id="rd" class="section md-padding">
        <div class="container">
            <div class="row">
                <div class="section-header text-center">
                    <h2 class="title">{{#t}}Research & Development{{/t}}</h2>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-diamond"></i>
                        <h3>{{#t}}pcb_design{{/t}}</h3>
                        <p>{{#t}}pcb_design_text{{/t}}</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-cogs"></i>
                        <h3>{{#t}}cad_modeling{{/t}}</h3>
                        <p>{{#t}}cad_modeling_text{{/t}}</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-laptop"></i>
                        <h3>{{#t}}software_development{{/t}}</h3>
                        <p>{{#t}}software_development_text{{/t}}</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-microchip"></i>
                        <h3>{{#t}}firmware_development{{/t}}</h3>
                        <p>{{#t}}firmware_development_text{{/t}}</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-superscript"></i>
                        <h3>{{#t}}algorithm_development{{/t}}</h3>
                        <p>{{#t}}algorithm_development_text{{/t}}</p>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="service">
                        <i class="fa fa-print"></i>
                        <h3>{{#t}}3d_printing{{/t}}</h3>
                        <p>{{#t}}3d_printing_text{{/t}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div id="numbers" class="section sm-padding">
        <div class="bg-img" style="background-image: url('./img/backgrounds/01.jpg');">
            <div class="overlay"></div>
        </div>

        <div class="container">
            <div class="row">
                <div class="col-sm-3 col-xs-6">
                    <div class="number">
                        <i class="fa fa-clock-o"></i>
                        <h3 class="white-text"><span class="counter">10+</span></h3>
                        <span class="white-text">{{#t}}Years of Experience{{/t}}</span>
                    </div>
                </div>
                <div class="col-sm-3 col-xs-6">
                    <div class="number">
                        <i class="fa fa-cogs"></i>
                        <h3 class="white-text"><span class="counter">20+</span></h3>
                        <span class="white-text">{{#t}}Assembled Robots{{/t}}</span>
                    </div>
                </div>
                <div class="col-sm-3 col-xs-6">
                    <div class="number">
                        <i class="fa fa-user"></i>
                        <h3 class="white-text"><span class="counter">10+</span></h3>
                        <span class="white-text">{{#t}}Current Team Members{{/t}}</span>
                    </div>
                </div>
                <div class="col-sm-3 col-xs-6">
                    <div class="number">
                        <i class="fa fa-graduation-cap"></i>
                        <h3 class="white-text"><span class="counter">100+</span></h3>
                        <span class="white-text">{{#t}}Students{{/t}}</span>
                    </div>
                </div>
            </div>
        </div>
    </div>


	<div id="gallery" class="section md-padding bg-grey">
		<div class="container">
			<div class="row">
				<div class="section-header text-center">
					<h2 class="title">{{#t}}Gallery{{/t}}</h2>
				</div>
                		<div class="elfsight-app-6a155035-5c52-48e9-8a7c-5923ee871179"></div>
			</div>
		</div>
	</div>


